    "start:pm2": "pm2 start pm2.config.js",
    "start:pm2:prod": "pm2 start pm2.config.js --env=production",
    "stop:pm2": "pm2 stop pm2.config.js",
    "delete:pm2": "pm2 delete pm2.config.js"
